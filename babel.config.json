{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "defaults",
        "modules": "cjs",
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [["babel-plugin-add-import-extension", { "extension": "js", "replace": true }]]
}
